<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mingsheng.mapper.RecoveryOrderMapper">
 
<insert id="addOrder" parameterType="com.mingsheng.model.RecoveryOrder">
INSERT INTO `recoveryorder` (`id`, `orderNo`, `userId`, `address`, `orderStatus`, `remark`, `phone`, `isDel`, `ctime`, `mobileType`, `mobileName`, `mobileColour`, `name`)
VALUES
	(#{id}, #{orderNo}, #{userId}, #{address}, #{orderStatus}, #{remark}, #{remark}, 0, #{ctime}, #{mobileType}, #{mobileName}, #{mobileColour}, #{name});
</insert>
 

</mapper>